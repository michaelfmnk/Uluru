{
  "name": "AldrinUI",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && webpack --mode production --config ./webpack.config.dev.js -p --color --progress && npm run po2json",
    "start": "rm -rf dist/lang && npm run po2json && webpack-dev-server --mode development --config ./webpack.config.dev.js --content-base dist --debug --output-pathinfo --watch --inline --port 8100",
    "startLight": "webpack-dev-server --mode development --config ./webpack.config.dev.js --content-base dist --debug --output-pathinfo --watch --inline --port 8100",
    "test": "jest",
    "nodemon": "cross-env BABEL_ENV='node' nodemon server/runner.js",
    "extract_lang": "./bin/extract_lang.sh",
    "po2json": "./bin/po2json.sh"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/datasets/",
      "/node_modules/"
    ],
    "timers": "fake",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 55,
        "functions": 50
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.1.1",
    "@material-ui/icons": "^3.0.1",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "dateformat": "^3.0.3",
    "file-saver": "^1.3.3",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "jed": "^1.1.1",
    "material-ui-pickers": "^1.0.0-rc.9",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.1",
    "query-string": "^5.1.0",
    "react": "^16.3.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-autosuggest": "^9.3.4",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.3.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-infinite-scroller": "^1.1.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^4.3.3",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.9",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.2",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "po2json": "^0.4.5",
    "react-hot-loader": "^3.1.3",
    "redux-mock-store": "^1.5.1",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.0.0",
    "webpack-hot-middleware": "^2.21.0"
  }
}
